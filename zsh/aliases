alias update-env="~/dotfiles/update_env.sh"
alias edit-zshrc="vim ~/dotfiles/zsh/.zshrc"
alias edit-alias="vim ~/dotfiles/zsh/aliases"

# youtube-download aliases to download as mp3
alias ytdl-mp3="youtube-dl --extract-audio --audio-format mp3 --audio-quality 128K --prefer-ffmpeg -o \"%(title)s.%(ext)s\""
alias ytdl-ch-mp3="ytdl-mp3 --continue --ignore-errors --no-overwrites"
